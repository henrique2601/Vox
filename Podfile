# platform :ios, '9.0'

target 'Vox' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Vox
  pod 'Firebase/Database'	
  pod 'Firebase/Auth'  
  pod 'Firebase/Storage' 
  pod 'Firebase/Core'
  pod 'Firebase/Firestore' 

  target 'VoxTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'VoxUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
